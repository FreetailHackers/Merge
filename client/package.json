{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mantine/core": "^5.7.1",
    "@mantine/hooks": "^5.7.1",
    "axios": "^0.22.0",
    "buffer": "^6.0.3",
    "buil": "^0.0.0",
    "crypto-browserify": "^3.12.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode-non-json": "^3.1.2",
    "miragejs": "^0.1.41",
    "patch-package": "^6.4.7",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "^5.0.0",
    "socket.io-client": "^4.0.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4"
  },
  "scripts": {
    "start": " react-scripts start",
    "start-windows": "set PORT=5000 && cross-env REACT_APP_API_URL=$npm_config_api REACT_APP_CHAT_URL=$npm_config_chat_api react-scripts start",
    "debug_api_string": "echo $npm_config_api",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "format": "npx prettier --write '**/*.js'",
    "lint": "npx eslint '**/*.js'",
    "postinstall": "patch-package"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl|jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testEnvironment": "jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.30.1",
    "jest": "^28.1.3",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.7.1"
  }
}
